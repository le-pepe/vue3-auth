{
  "name": "@le-pepe/vue-auth",
  "version": "0.0.6",
  "description": "Vue 3 plugin for handling JWT authentication with optional refresh tokens",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "prepare": "npm run build",
    "preview": "vite preview",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/le-pepe/vue3-auth.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "authentication",
    "auth",
    "jwt",
    "refresh-token",
    "vue-router"
  ],
  "author": "Le Pepe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/le-pepe/vue3-auth/issues"
  },
  "homepage": "https://github.com/le-pepe/vue3-auth#readme",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "standard-version": "^9.5.0",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cookie-store": "4.0.0-next.4"
  },
  "peerDependencies": {
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
